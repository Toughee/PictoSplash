{
  "name": "imagebox",
  "version": "1.5.0",
  "description": "Image search engine web app created in React. The app uses the Unsplash API to display multiple images at the same time and perform real-time search queries inside of it.",
  "main": "index.js",
  "homepage": "https://tjdev7.github.io/ImageBox/",
